    "community.general.purefb_facts": {
        "doc": {
            "author": [
                "Pure Storage Ansible Team (@sdodsley) <pure-ansible-team@purestorage.com>"
            ],
            "collection": "community.general",
            "deprecated": {
                "alternative": "Use M(purestorage.flashblade.purefb_info) instead.",
                "removed_from_collection": "community.general",
                "removed_in": "3.0.0",
                "why": "Deprecated in favor of C(_info) module."
            },
            "description": [
                "Collect facts information from a Pure Storage FlashBlade running the Purity//FB operating system. By default, the module will collect basic fact information including hosts, host groups, protection groups and volume counts. Additional fact information can be collected based on the configured set of arguments."
            ],
            "filename": "/Users/ssbarnea/.pyenv/versions/3.9.2/lib/python3.9/site-packages/ansible_collections/community/general/plugins/modules/purefb_facts.py",
            "has_action": false,
            "module": "purefb_facts",
            "notes": [
                "This module requires the C(purity_fb) Python library",
                "You must set C(PUREFB_URL) and C(PUREFB_API) environment variables if I(fb_url) and I(api_token) arguments are not passed to the module directly"
            ],
            "options": {
                "api_token": {
                    "description": [
                        "FlashBlade API token for admin privileged user."
                    ],
                    "type": "str"
                },
                "fb_url": {
                    "description": [
                        "FlashBlade management IP address or Hostname."
                    ],
                    "type": "str"
                },
                "gather_subset": {
                    "default": "minimum",
                    "description": [
                        "When supplied, this argument will define the facts to be collected. Possible values for this include all, minimum, config, performance, capacity, network, subnets, lags, filesystems and snapshots."
                    ],
                    "required": false,
                    "type": "list"
                }
            },
            "requirements": [
                "python >= 2.7",
                "purity_fb >= 1.1"
            ],
            "short_description": "Collect facts from Pure Storage FlashBlade"
        },
        "examples": "\n- name: Collect default set of facts\n  community.general.purefb_facts:\n    fb_url: 10.10.10.2\n    api_token: T-55a68eb5-c785-4720-a2ca-8b03903bf641\n\n- name: Collect configuration and capacity facts\n  community.general.purefb_facts:\n    gather_subset:\n      - config\n      - capacity\n    fb_url: 10.10.10.2\n    api_token: T-55a68eb5-c785-4720-a2ca-8b03903bf641\n\n- name: Collect all facts\n  community.general.purefb_facts:\n    gather_subset:\n      - all\n    fb_url: 10.10.10.2\n    api_token: T-55a68eb5-c785-4720-a2ca-8b03903bf641\n",
