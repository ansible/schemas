                    "type": "dict"
                },
                "region": {
                    "choices": [
                        "ams1",
                        "EMEA-NL-EVS",
                        "par1",
                        "EMEA-FR-PAR1",
                        "par2",
                        "EMEA-FR-PAR2",
                        "waw1",
                        "EMEA-PL-WAW1"
                    ],
                    "description": [
                        "Scaleway region to use (for example par1)."
                    ],
                    "required": true,
                    "type": "str"
                },
                "validate_certs": {
                    "default": true,
                    "description": [
                        "Validate SSL certs of the Scaleway API."
                    ],
                    "type": "bool"
                }
            },
            "short_description": "Gather facts about the Scaleway servers available."
        },
